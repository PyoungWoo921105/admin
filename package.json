  "scripts": {
    "start": "dotenv -e env/.env.develop craco start",
    "start:develop": "set \"GENERATE_SOURCEMAP=false\" && dotenv -e env/.env.develop react-scripts start",
    "build:develop": "set \"GENERATE_SOURCEMAP=false\" && dotenv -e env/.env.develop react-scripts build",
    "start:qa": "set \"GENERATE_SOURCEMAP=false\" && dotenv -e env/.env.qa craco start",
    "build:qa": "set \"GENERATE_SOURCEMAP=false\" && dotenv -e env/.env.qa react-scripts build",
    "start:production": "set \"GENERATE_SOURCEMAP=false\" && dotenv -e env/.env.production react-scripts start",
    "build:production": "set \"GENERATE_SOURCEMAP=false\" && dotenv -e env/.env.production react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.20.0",
